from tkinder import tk

class BackupTool():
    def __init__(self, root):
        self.root = root
        self.root.title("LTE/NR Log Backup Tool")
        self.geometry('500x600')
        self.root.resizable(True, True)
        
        self.running = False
        
        self.mode = tk.StringVar(value="LTE")



if __name__=="__main__":
root = tk.TK()
app = BackupTool(root)
root.mainloop()
